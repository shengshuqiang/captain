<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="captain">

<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
<!--<uses-permission android:name="android.permission.USE_FINGERPRINT" />-->

<application
    android:name="com.shuqiang.captain.CaptainApplication"
    android:allowBackup="true"
    android:icon="@drawable/captain"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:requestLegacyExternalStorage="true"
    android:theme="@style/AppTheme.NoActionBar">

    <activity
        android:name="com.shuqiang.captain.MainActivity"
        android:label="船长"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity
        android:name="com.shuqiang.captain.qr.QRActivity"
        android:label="信息二维马">

    </activity>

    <activity
        android:name="com.shuqiang.captain.qr.test.QRTestActivity"
        android:exported="true"
        android:label="信息二维马测试页"
        >
    </activity>

</application>

</manifest>