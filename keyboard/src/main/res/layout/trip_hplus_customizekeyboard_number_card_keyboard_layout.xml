<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:keyHeight="50dp"
          android:horizontalGap="1px"
          android:verticalGap="1px"
          android:keyWidth="33.33%p">

    <Row>

        <Key
            android:codes="49"
            android:keyEdgeFlags="left"
            android:keyLabel="1"/>

        <Key
            android:codes="50"
            android:keyLabel="2"/>

        <Key
            android:codes="51"
            android:keyLabel="3"/>
    </Row>

    <Row>

        <Key
            android:codes="52"
            android:keyEdgeFlags="left"
            android:keyLabel="4"/>

        <Key
            android:codes="53"
            android:keyLabel="5"/>

        <Key
            android:codes="54"
            android:keyLabel="6"/>

    </Row>
    <Row>

        <Key
            android:codes="55"
            android:keyEdgeFlags="left"
            android:keyLabel="7"/>

        <Key
            android:codes="56"
            android:keyLabel="8"/>

        <Key
            android:codes="57"
            android:keyLabel="9"/>
    </Row>

    <Row>

        <Key
            android:codes="88"
            android:keyEdgeFlags="left"/>

        <Key
            android:codes="48"
            android:keyLabel="0"/>

        <!--删除按键长按时连续响应-->
        <Key
            android:codes="60001"
            android:isRepeatable="true"
            android:keyIcon="@drawable/trip_hplus_customizekeyboard_keyboard_del"/>
    </Row>

</Keyboard>